{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmSize": {
      "type": "string",
      "allowedValues": [
        "Standard_DS3_v2",
        "Standard_DS4_v2",
        "Standard_DS5_v2"
      ],
      "metadata": {
        "description": "Azure VM size for VM-Series"
      }
    },
    "productoffer": {
      "type": "string",
      "defaultValue": "vmseries1",
      "allowedValues": [
        "vmseries1",
        "vmseries1-beta-preview"
      ],
      "metadata": {
        "description": "Deploy with Standard or Budapest Beta"
      }
    },
    "pafwversion": {
      "type": "string",
      "defaultValue": "8.0.0",
      "allowedValues": [
        "8.0.0",
        "8.1.0"
      ],
      "metadata": {
        "description": "Deploy versions with Standard 8.0.0 or Budapest Beta 8.1.0"
      }
    },
    "namesku": {
      "type": "string",
      "defaultValue": "byol",
      "allowedValues": [
        "byol",
        "bundle1",
        "bundle2"
      ],
      "metadata": {
        "description": "Your cost liscening type byol or paygo"
      }
    },
    "vmsscount": {
      "defaultValue": 2,
      "type": "int",
      "metadata": {
        "description": "How many PA FWs to deploy in VMSS"
      }
    },
    "Username": {
      "defaultValue": "paloalto",
      "type": "string"
    },
    "Password": {
      "defaultValue": null,
      "type": "securestring"
    }
  },
  "variables": {
    "uniqueId": "[uniqueString(resourceGroup().id)]"
  },
  "resources": [
    {
      "type": "Microsoft.Network/routeTables",
      "name": "RT-Web",
      "apiVersion": "2016-03-30",
      "location": "[resourceGroup().location]",
      "properties": {
        "routes": [
          {
            "name": "Webroute-Network",
            "properties": {
              "addressPrefix": "10.0.0.0/16",
              "nextHopType": "VirtualAppliance",
              "nextHopIpAddress": "10.0.3.100"
            }
          },
          {
            "name": "Webroute-Local",
            "properties": {
              "addressPrefix": "10.0.4.0/24",
              "nextHopType": "VnetLocal"
            }
          },
          {
            "name": "Webroute-Default",
            "properties": {
              "addressPrefix": "0.0.0.0/0",
              "nextHopType": "VirtualAppliance",
              "nextHopIpAddress": "10.0.3.100"
            }
          },
          {
            "name": "Webroute-Trust",
            "properties": {
              "addressPrefix": "10.0.2.0/24",
              "nextHopType": "VnetLocal"
            }
          }
        ]
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "apiVersion": "2016-06-01",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "NET-Demo",
      "location": "[resourceGroup().location]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "10.0.0.0/16"
          ]
        },
        "subnets": [
          {
            "name": "Management",
            "properties": {
              "addressPrefix": "10.0.0.0/24",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', 'Allow-All')]"
              }
            }
          },
          {
            "name": "Untrust",
            "properties": {
              "addressPrefix": "10.0.1.0/24",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', 'Allow-All')]"
              }
            }
          },
          {
            "name": "Trust",
            "properties": {
              "addressPrefix": "10.0.2.0/24",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', 'Allow-All')]"
              }
            }
          },
          {
            "name": "Egress",
            "properties": {
              "addressPrefix": "10.0.3.0/24",
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', 'Allow-All')]"
              }
            }
          },
          {
            "name": "Webservers",
            "properties": {
              "addressPrefix": "10.0.4.0/24",
              "routeTable": {
                "id": "[resourceId('Microsoft.Network/routeTables', 'RT-Web')]"
              },
              "networkSecurityGroup": {
                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', 'Allow-All')]"
              }
            }
          }
        ]
      },
      "dependsOn": [
        "[resourceId('Microsoft.Network/routeTables', 'RT-Web')]"
      ]
    },
    {
      "type": "Microsoft.Network/publicIPAddresses",
      "name": "IP-LB-Public",
      "apiVersion": "2017-08-01",
      "location": "[resourceGroup().location]",
      "sku": {
        "name": "Standard"
      },
      "properties": {
        "publicIPAllocationMethod": "Static",
        "idleTimeoutInMinutes": 4,
        "dnsSettings": {
          "domainNameLabel": "[concat('lbfw', variables('uniqueId'))]"
        }
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "type": "Microsoft.Network/loadBalancers",
      "name": "LB-Web",
      "apiVersion": "2017-08-01",
      "location": "[resourceGroup().location]",
      "sku": {
        "name": "Standard"
      },
      "properties": {
        "frontendIPConfigurations": [
          {
            "name": "LB-Web-FrontEnd",
            "properties": {
              "privateIPAddress": "10.0.4.100",
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Webservers')]"
              }
            }
          }
        ],
        "backendAddressPools": [
          {
            "name": "AS-Web"
          }
        ],
        "loadBalancingRules": [
          {
            "name": "LB-Web-HTTP",
            "properties": {
              "frontendIPConfiguration": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Web'), '/frontendIPConfigurations/LB-Web-FrontEnd')]"
              },
              "frontendPort": 80,
              "backendPort": 80,
              "enableFloatingIP": false,
              "idleTimeoutInMinutes": 4,
              "protocol": "Tcp",
              "loadDistribution": "Default",
              "backendAddressPool": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Web'), '/backendAddressPools/AS-Web')]"
              },
              "probe": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Web'), '/probes/HTTP-80')]"
              }
            }
          }
        ],
        "probes": [
          {
            "name": "HTTP-80",
            "properties": {
              "protocol": "Http",
              "port": 80,
              "requestPath": "/",
              "intervalInSeconds": 5,
              "numberOfProbes": 2
            }
          }
        ],
        "inboundNatRules": [],
        "outboundNatRules": [],
        "inboundNatPools": []
      },
      "resources": [],
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo')]",
        "[resourceId('Microsoft.Compute/availabilitySets', 'AS-Web')]"
      ]
    },
    {
      "type": "Microsoft.Network/loadBalancers",
      "name": "LB-Public",
      "apiVersion": "2017-08-01",
      "location": "[resourceGroup().location]",
      "sku": {
        "name": "Standard"
      },
      "properties": {
        "frontendIPConfigurations": [
          {
            "name": "LB-Public-FrontEnd",
            "properties": {
              "privateIPAllocationMethod": "Dynamic",
              "publicIPAddress": {
                "id": "[resourceId('Microsoft.Network/publicIPAddresses', 'IP-LB-Public')]"
              }
            }
          }
        ],
        "backendAddressPools": [
          {
            "name": "AS-FW"
          }
        ],
        "loadBalancingRules": [
          {
            "name": "LB-Public-HTTP",
            "properties": {
              "frontendIPConfiguration": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Public'), '/frontendIPConfigurations/Lb-Public-FrontEnd')]"
              },
              "frontendPort": 80,
              "backendPort": 80,
              "enableFloatingIP": false,
              "idleTimeoutInMinutes": 4,
              "protocol": "Tcp",
              "loadDistribution": "Default",
              "backendAddressPool": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Public'), '/backendAddressPools/AS-FW')]"
              },
              "probe": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Public'), '/probes/TCP-22')]"
              }
            }
          }
        ],
        "probes": [
          {
            "name": "TCP-22",
            "properties": {
              "protocol": "Tcp",
              "port": 22,
              "intervalInSeconds": 5,
              "numberOfProbes": 2
            }
          }
        ],
        "inboundNatRules": [],
        "outboundNatRules": [],
        "inboundNatPools": []
      },
      "resources": [],
      "dependsOn": [
        "[resourceId('Microsoft.Network/publicIPAddresses', 'IP-LB-Public')]",
        "[resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo')]"
      ]
    },
    {
      "type": "Microsoft.Network/loadBalancers",
      "name": "LB-Egress",
      "apiVersion": "2017-08-01",
      "location": "[resourceGroup().location]",
      "sku": {
        "name": "Standard"
      },
      "properties": {
        "frontendIPConfigurations": [
          {
            "name": "LB-Egress-FrontEnd",
            "properties": {
              "privateIPAddress": "10.0.3.100",
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Egress')]"
              }
            }
          }
        ],
        "backendAddressPools": [
          {
            "name": "AS-FW"
          }
        ],
        "loadBalancingRules": [
          {
            "name": "Allow-All-Out",
            "properties": {
              "frontendIPConfiguration": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Egress'), '/frontendIPConfigurations/LB-Egress-FrontEnd')]"
              },
              "frontendPort": 0,
              "backendPort": 0,
              "enableFloatingIP": true,
              "idleTimeoutInMinutes": 4,
              "protocol": "All",
              "loadDistribution": "Default",
              "backendAddressPool": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Egress'), '/backendAddressPools/AS-FW')]"
              },
              "probe": {
                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Egress'), '/probes/TCP-22')]"
              }
            }
          }
        ],
        "probes": [
          {
            "name": "TCP-22",
            "properties": {
              "protocol": "Tcp",
              "port": 22,
              "intervalInSeconds": 5,
              "numberOfProbes": 2
            }
          }
        ],
        "inboundNatRules": [],
        "outboundNatRules": [],
        "inboundNatPools": []
      },
      "resources": [],
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo')]"
      ]
    },
    {
      "apiVersion": "2015-06-15",
      "type": "Microsoft.Network/networkInterfaces",
      "name": "INT-Web1",
      "location": "[resourceGroup().location]",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo')]",
        "[resourceId('Microsoft.Network/loadBalancers', 'LB-Web')]"
      ],
      "properties": {
        "enableIPForwarding": false,
        "ipConfigurations": [
          {
            "name": "Web1",
            "properties": {
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Webservers')]"
              },
              "privateIpAddress": "10.0.4.50",
              "loadBalancerBackendAddressPools": [
                {
                  "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Web'), '/backendAddressPools/AS-Web')]"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "apiVersion": "2015-06-15",
      "type": "Microsoft.Network/networkInterfaces",
      "name": "INT-Web2",
      "location": "[resourceGroup().location]",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo')]",
        "[resourceId('Microsoft.Network/loadBalancers', 'LB-Web')]"
      ],
      "properties": {
        "enableIPForwarding": false,
        "ipConfigurations": [
          {
            "name": "Web2",
            "properties": {
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Webservers')]"
              },
              "privateIpAddress": "10.0.4.51",
              "loadBalancerBackendAddressPools": [
                {
                  "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Web'), '/backendAddressPools/AS-Web')]"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.Network/networkSecurityGroups",
      "name": "Allow-All",
      "apiVersion": "2017-06-01",
      "location": "[resourceGroup().location]",
      "properties": {
        "securityRules": [
          {
            "name": "Allow-All-In",
            "properties": {
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 100,
              "direction": "Inbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "Allow-All-Out",
            "properties": {
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 100,
              "direction": "Outbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          }
        ],
        "defaultSecurityRules": [
          {
            "name": "AllowVnetInBound",
            "properties": {
              "description": "Allow inbound traffic from all VMs in VNET",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "VirtualNetwork",
              "destinationAddressPrefix": "VirtualNetwork",
              "access": "Allow",
              "priority": 65000,
              "direction": "Inbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "AllowAzureLoadBalancerInBound",
            "properties": {
              "description": "Allow inbound traffic from azure load balancer",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "AzureLoadBalancer",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 65001,
              "direction": "Inbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "DenyAllInBound",
            "properties": {
              "description": "Deny all inbound traffic",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Deny",
              "priority": 65500,
              "direction": "Inbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "AllowVnetOutBound",
            "properties": {
              "description": "Allow outbound traffic from all VMs to all VMs in VNET",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "VirtualNetwork",
              "destinationAddressPrefix": "VirtualNetwork",
              "access": "Allow",
              "priority": 65000,
              "direction": "Outbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "AllowInternetOutBound",
            "properties": {
              "description": "Allow outbound traffic from all VMs to Internet",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "Internet",
              "access": "Allow",
              "priority": 65001,
              "direction": "Outbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          },
          {
            "name": "DenyAllOutBound",
            "properties": {
              "description": "Deny all outbound traffic",
              "protocol": "*",
              "sourcePortRange": "*",
              "destinationPortRange": "*",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Deny",
              "priority": 65500,
              "direction": "Outbound",
              "sourceAddressPrefixes": [],
              "destinationAddressPrefixes": []
            }
          }
        ]
      },
      "dependsOn": []
    },
    {
      "type": "Microsoft.Compute/availabilitySets",
      "name": "AS-Web",
      "apiVersion": "2016-04-30-preview",
      "location": "[resourceGroup().location]",
      "tags": {},
      "properties": {
        "platformUpdateDomainCount": 2,
        "platformFaultDomainCount": 2
      },
      "sku": {
        "name": "Aligned"
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "type": "Microsoft.Compute/virtualMachineScaleSets",
      "name": "PAFWScaleSet",
      "location": "[resourceGroup().location]",
      "apiVersion": "2017-12-01",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo')]",
        "[resourceId('Microsoft.Network/loadBalancers', 'LB-Web')]",
        "[resourceId('Microsoft.Network/loadBalancers', 'LB-Public')]",
        "[resourceId('Microsoft.Network/loadBalancers', 'LB-Egress')]"
      ],
      "sku": {
        "name": "[parameters('vmSize')]",
        "capacity": "[parameters('vmsscount')]"
      },
      "plan": {
        "name": "[parameters('namesku')]",
        "product": "[parameters('productoffer')]",
        "publisher": "paloaltonetworks"
      },
      "properties": {
        "upgradePolicy": {
          "mode": "Manual"
        },
        "virtualMachineProfile": {
          "storageProfile": {
            "imageReference": {
              "publisher": "paloaltonetworks",
              "offer": "[parameters('productoffer')]",
              "sku": "[parameters('namesku')]",
              "version": "[parameters('pafwversion')]"
            }
          },
          "osProfile": {
            "computerNamePrefix": "VM-FW",
            "adminUsername": "[parameters('Username')]",
            "adminPassword": "[parameters('Password')]"
          },
          "networkProfile": {
            "networkInterfaceConfigurations": [
              {
                "name": "INT-FW-Management",
                "properties": {
                  "primary": "false",
                  "enableAcceleratedNetworking": false,
                  "ipConfigurations": [
                    {
                      "name": "FW-Management",
                      "properties": {
                        "subnet": {
                          "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Management')]"
                        },
                        "publicIpAddressConfiguration": {
                          "name": "PIP-FW-Management",
                          "properties": {
                            "idleTimeoutInMinutes": 15,
                            "dnsSettings": {
                              "domainNameLabel": "[concat('fwmanagement', variables('uniqueId'))]"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              },
              {
                "name": "INT-FW-Untrust",
                "properties": {
                  "primary": "true",
                  "enableAcceleratedNetworking": false,
                  "enableIPForwarding": true,
                  "ipConfigurations": [
                    {
                      "name": "FW-Untrust",
                      "properties": {
                        "subnet": {
                          "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Untrust')]"
                        },
                        "publicIpAddressConfiguration": {
                          "name": "PIP-FW-Egress",
                          "properties": {
                            "idleTimeoutInMinutes": 15,
                            "dnsSettings": {
                              "domainNameLabel": "[concat('fwuntrust', variables('uniqueId'))]"
                            }
                          }
                        },
                        "loadBalancerBackendAddressPools": [
                          {
                            "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Public'), '/backendAddressPools/AS-FW')]"
                          }
                        ]
                      }
                    }
                  ]
                }
              },
              {
                "name": "INT-FW-Trust",
                "properties": {
                  "primary": "false",
                  "enableAcceleratedNetworking": false,
                  "enableIPForwarding": true,
                  "ipConfigurations": [
                    {
                      "name": "FW-Trust",
                      "properties": {
                        "subnet": {
                          "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', 'NET-Demo'), '/subnets/Trust')]"
                        },
                        "loadBalancerBackendAddressPools": [
                          {
                            "id": "[concat(resourceId('Microsoft.Network/loadBalancers', 'LB-Egress'), '/backendAddressPools/AS-FW')]"
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "2016-04-30-preview",
      "type": "Microsoft.Compute/virtualMachines",
      "name": "VM-Web1",
      "location": "[resourceGroup().location]",
      "dependsOn": [
        "[resourceId('Microsoft.Network/networkInterfaces/', 'INT-Web1')]",
        "[resourceId('Microsoft.Compute/availabilitySets', 'AS-Web')]"
      ],
      "properties": {
        "availabilitySet": {
          "id": "[resourceId('Microsoft.Compute/availabilitySets', 'AS-Web')]"
        },
        "hardwareProfile": {
          "vmSize": "Standard_F1s"
        },
        "osProfile": {
          "computerName": "VM-Web1",
          "adminUsername": "[parameters('Username')]",
          "adminPassword": "[parameters('Password')]"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "MicrosoftWindowsServer",
            "offer": "WindowsServer",
            "sku": "2016-Datacenter",
            "version": "latest"
          },
          "osDisk": {
            "managedDisk": {
              "storageAccountType": "Standard_LRS"
            },
            "caching": "ReadWrite",
            "createOption": "FromImage"
          },
          "dataDisks": [
            {
              "diskSizeGB": "10",
              "lun": 0,
              "managedDisk": {
                "storageAccountType": "Standard_LRS"
              },
              "createOption": "Empty"
            }
          ]
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', 'INT-Web1')]"
            }
          ]
        },
        "diagnosticsProfile": {
          "bootDiagnostics": {
            "enabled": "false"
          }
        }
      }
    },
    {
      "apiVersion": "2016-04-30-preview",
      "type": "Microsoft.Compute/virtualMachines",
      "name": "VM-Web2",
      "location": "[resourceGroup().location]",
      "dependsOn": [
        "[resourceId('Microsoft.Network/networkInterfaces/', 'INT-Web2')]",
        "[resourceId('Microsoft.Compute/availabilitySets', 'AS-Web')]"
      ],
      "properties": {
        "availabilitySet": {
          "id": "[resourceId('Microsoft.Compute/availabilitySets', 'AS-Web')]"
        },
        "hardwareProfile": {
          "vmSize": "Standard_F1s"
        },
        "osProfile": {
          "computerName": "VM-Web2",
          "adminUsername": "[parameters('Username')]",
          "adminPassword": "[parameters('Password')]"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "MicrosoftWindowsServer",
            "offer": "WindowsServer",
            "sku": "2016-Datacenter",
            "version": "latest"
          },
          "osDisk": {
            "managedDisk": {
              "storageAccountType": "Standard_LRS"
            },
            "caching": "ReadWrite",
            "createOption": "FromImage"
          },
          "dataDisks": [
            {
              "diskSizeGB": "10",
              "lun": 0,
              "managedDisk": {
                "storageAccountType": "Standard_LRS"
              },
              "createOption": "Empty"
            }
          ]
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', 'INT-Web2')]"
            }
          ]
        },
        "diagnosticsProfile": {
          "bootDiagnostics": {
            "enabled": "false"
          }
        }
      }
    }
  ],
  "outputs": {
    "public pip lb": {
      "type": "String",
      "value": "[reference('IP-LB-Public').dnsSettings.fqdn]"
    }
  }
